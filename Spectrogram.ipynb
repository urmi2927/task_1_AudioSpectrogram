   "source": [
    "import librosa\n",
    "audio = 'data.wav'\n",
    "x, sr = librosa.load(audio)\n",
    "X = librosa.stft(x)\n",
    "Xdb = librosa.amplitude_to_db(abs(X))\n",
    "plt.figure(figsize = (10, 5))\n",
    "librosa.display.specshow(Xdb, sr = sr, x_axis = 'time', y_axis = 'hz')\n",
    "plt.colorbar()"
   ]
  }
